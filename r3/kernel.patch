- {
- 	struct b53_device *dev = ds->priv;
- 
--	if (is5325(dev) || is5365(dev))
--		return -EOPNOTSUPP;
-+	return !is5325(dev) && !is5365(dev);
-+}
-+EXPORT_SYMBOL(b53_support_eee);
- 
-+int b53_get_mac_eee(struct dsa_switch *ds, int port, struct ethtool_keee *e)
